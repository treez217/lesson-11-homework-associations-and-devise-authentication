<h1> Tag List </h1>

<%=link_to 'Create New Tag', new_tag_path%>

<% @tags.each do |tag| %>

  <table>
    <td><%=link_to 'Delete', tag_path(tag), method: :delete, data: {conirm: 'Yes No'}%></td>
    <td><%=link_to 'Edit', edit_tag_path(tag) %></td>
    <td><%=link_to tag.name, tag_path(tag)%> </td>

  </table>

<% end %>